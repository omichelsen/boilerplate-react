{
  "compilerOptions": {
    "jsx": "react",
    "lib": [
      "dom",
      "es7",
      "es2017.object"
    ],
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./public/",
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./src/**/*.test.*",
    "node_modules"
  ]
}
