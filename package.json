{
  "name": "boilerplate-react",
  "version": "1.0.0",
  "description": "Boilerplate with React+Webpack",
  "repository": {
    "type": "git",
    "url": "git@github.com:omichelsen/boilerplate-react.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "rimraf public && webpack --env production",
    "build:stats": "rimraf public && webpack --env production --json > stats.json",
    "coverage": "cross-env NODE_ENV=test nyc npm test --reporter progress",
    "lint": "eslint --ext .jsx,.js .",
    "start": "webpack-dev-server",
    "test": "mocha",
    "test:watch": "mocha --watch --reporter min"
  },
  "author": "Ole Michelsen",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "deepmerge": "^1.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "istanbul": "^0.4.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "offline-plugin": "^4.8.3",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "react-addons-test-utils": "^15.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "web-app-manifest-loader": "^0.1.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">=7"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js*",
      "test"
    ]
  }
}
